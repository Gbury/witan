(jbuild_version 1)

; main binary
(executable
  ((name main)
   (public_name witan)
   (libraries (containers))
   (flags (:standard -w +a-4-42-44-48-50-58-32-60@8 -color always))
   (ocamlopt_flags (:standard -O3 -color always
                    -unbox-closures -unbox-closures-factor 20))
  ))

(alias
 ((name    runtest)
  (deps    (tests/test.exe))
  (action  (run ${<}))))
