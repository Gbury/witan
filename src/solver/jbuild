(jbuild_version 1)

(library
  ((name witan_solver)
   (public_name witan.solver)
   (synopsis "witan's solver")
   (libraries (containers zarith ocamlgraph
               gen dolmen spelll
               witan.open
               witan.stdlib
               witan.popop_lib str
               witan.core
               witan.core.structures))
   (preprocess (pps (ppx_deriving.std)))
   (flags (:standard -w +a-4-42-44-48-50-58-32-60-40-9@8 -color always -open Witan_stdlib -open Witan_popop_lib -open Witan_core))
   (ocamlopt_flags (:standard -O3 -bin-annot
                    -unbox-closures -unbox-closures-factor 20))
  ))
