(jbuild_version 1)

(library
  ((name witan_core)
   (public_name witan.core)
   (synopsis "core mcsat solver for witan")
   (libraries (containers zarith ocamlgraph witan.stdlib witan.open syntax witan.popop_lib str))
   (modules (:standard \ open))
   (preprocess (pps (ppx_deriving.std)))
   (flags (:standard -w +a-4-42-44-48-50-58-32-60-40-9@8 -color always -open Witan_stdlib -open Witan_popop_lib))
   (ocamlopt_flags (:standard -O3 -bin-annot
                    -unbox-closures -unbox-closures-factor 20))
  ))
