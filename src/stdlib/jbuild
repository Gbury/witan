(jbuild_version 1)

(library
  ((name witan_stdlib)
   (public_name witan.stdlib)
   (synopsis "Stdlib for witan")
   (libraries (zarith witan.open witan.popop_lib containers))
   (flags (:standard -w +a-4-42-44-48-50-58-32-60-40-9@8 -color always -open Witan_open.Open -open Witan_popop_lib))
   (ocamlopt_flags (:standard -O3 -bin-annot
                    -unbox-closures -unbox-closures-factor 20))
  ))
